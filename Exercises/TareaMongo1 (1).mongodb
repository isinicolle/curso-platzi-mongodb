        {
        $multiply:["$$this.precio","$$this.cantidad"]
}}}}
},
descuento:{
        $sum:{$sum:{$map:{"input":"$detalleventa", "in":
        {
        $multiply:["$$this.precio","$$this.cantidad",0.05]
}}}}
}

}
},{
$project:
{
anioslaborados:
{
$subtract:
["$total","$descuento"]
}
}
}])

